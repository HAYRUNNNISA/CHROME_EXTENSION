{
    "manifest_version": 3,
    "name": "Against Ads",
    "version": "1.0",
    "description": "A chrome extension that helps the user to skip ads amd enjoy their time without boring ads",
    "icons": {
      "128": "icon128.jpeg",
      "48":"icon48.jpeg",
      "16":"icon1.jpeg"
      
    },
    "background": {
      "scripts": ["extension.js"]
  },
    "action": {
      "default_icon": {
        
        "128": "icon128.jpeg",
        "48":"icon48.jpeg",
        "16":"icon1.jpeg"
      },
      "default_popup": "extension.html"
    }, 
   
    "permissions":
    ["tabs", "activeTab","notifications","webRequest", "webRequestBlocking"]
    

    
  }
{
    "manifest_version": 3,
    "name": "Video Downloader for free",
    "version": "1.0",
    "description": "A chrome extension for you to download your videos fast and free",
    "icons": {
      "128": "icon128.png",
      "48": "icon48.png",
      "16": "icon16.png"
    },
    "action": {
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      },
      "default_popup": "extension.html"
    },
    "background":{
      "service_worker":"extension.js"

    }
    

    
  }
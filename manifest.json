{
    "manifest_version": 3,
    "name": "AdSkipper",
    "version": "1.0",
    "description": "A chrome extension that helps the user to skip ads amd enjoy their time without boring ads",
    "icons": {
      "128": "icon128.jpeg",
      "48":"icon48.jpeg",
      "16":"icon1.jpeg"
      
    },
    "action": {
      "default_icon": {
        
        "128": "icon128.jpeg",
        "48":"icon48.jpeg",
        "16":"icon1.jpeg"
      },
      "default_popup": "extension.html"
    }, 
    "content_script":[{
      "js":["extension.js"],
      "run_at":"document_idle",
      "matches":["<all_urls>"]


    }],
    "permissions":
    ["tabs", "activeTab","notifications","webRequest", "webRequestBlocking"]
    

    
  }